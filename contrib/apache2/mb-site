<VirtualHost *:80>
    ServerName localhost
    DocumentRoot /home/guilhermeg/servers/pybossa/pybossa
    WSGIDaemonProcess pybossa user=guilhermeg group=lsd threads=5
    WSGIScriptAlias /pybossa /home/guilhermeg/servers/pybossa/contrib/pybossa.wsgi

    <Directory /home/guilhermeg/servers/pybossa>
        WSGIProcessGroup pybossa
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>

	WSGIDaemonProcess mb user=guilhermeg group=lsd threads=5
        WSGIScriptAlias /mb /local/pybossa_apps/app-tabletranscriber/contrib/app_tt.wsgi

    <Directory /local/pybossa_apps/app-tabletranscriber>
        WSGIProcessGroup mb
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>
</VirtualHost>
